{
    "name": "@saas/api",
    "version": "1.0.0",
    "scripts": {
    "dev": "npx dotenv -e ../../.env -- tsx watch src/http/server.ts",
    "db:migrate": "npx dotenv -e ../../.env -- npx prisma migrate dev",
    "db:studio": "npx dotenv -e ../../.env -- npx prisma studio",
    "build": "tsup",
    "start": "node dist/http/server.js"
    },
    "dependencies": {
      "@fastify/cors": "^9.0.1",
      "@fastify/jwt": "^8.0.0",
      "@fastify/swagger": "^8.14.0",
      "@fastify/swagger-ui": "^3.0.0",
      "@prisma/client": "5.12.1",
      "@saas/auth": "*",
      "@saas/env": "*",
      "bcryptjs": "^2.4.3",
      "fastify": "^4.26.2",
      "fastify-plugin": "^4.5.1",
      "fastify-type-provider-zod": "^1.1.9",
      "zod": "^3.22.4"
    },
    "devDependencies": {
      "@faker-js/faker": "^8.4.1",
      "@saas/eslint-config": "*",
      "@saas/prettier": "*",
      "@saas/tsconfig": "*",
      "@types/bcryptjs": "^2.4.6",
      "@types/node": "^20.12.6",
      "dotenv-cli": "^7.4.1",
      "prisma": "^5.12.1",
      "tsup": "^8.2.4",
      "tsx": "^4.7.2"
    },
    "prisma": {
      "seed": "tsx prisma/seed.ts"
    },
    "prettier": "@saas/prettier",
    "eslintConfig": {
      "extends": [
        "@saas/eslint-config/node"
      ]
    }
  }